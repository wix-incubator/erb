  "devDependencies": {
    "chai": "3.5.0",
    "eslint": "3.17.1",
    "eslint-plugin-mocha": "4.8.0",
    "mocha": "3.2.0",
    "mocha-env-reporter": "2.0.4",
    "npm-version-up": "0.1.0"
